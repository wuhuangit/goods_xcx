<view class="container" style="{{('height: '+docheight+'px;')}}"><view class="detaiimgbox"><image class="drtailimg" src="{{goodsobj.goods_imgs[0]}}"></image></view><view class="goodsinfo"><view class="goodsname">{{goodsobj.goods_name}}</view><view class="goodsprice"><view class="priceyuan">{{"￥"+goodsobj.goods_present_price}}</view><view class="pricexian"><text>{{"￥"+goodsobj.goods_original_price}}</text></view></view><view class="goodsdec">{{goodsobj.goods_describe}}</view><view class="goodsnum"><view data-event-opts="{{[['tap',[['jianfn',['$event']]]]]}}" class="addbox" bindtap="__e">-</view><view class="numinput"><input data-event-opts="{{[['input',[['__set_model',['','goodsnum','$event',[]]]]]]}}" value="{{goodsnum}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['addfn',['$event']]]]]}}" class="addbox" bindtap="__e">+</view></view></view><view class="footbox"><view class="allmoney">{{"总价："+$root.g0}}</view><view class="btnbox"><view data-event-opts="{{[['tap',[['addcarfn',['$event']]]]]}}" class="buybtn" bindtap="__e">加入购物车</view><view data-event-opts="{{[['tap',[['buyfn',['$event']]]]]}}" class="buybtn" bindtap="__e">立即购买</view></view></view></view>