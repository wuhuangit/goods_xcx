<view class="container"><view class="infobox"><view class="userimg"><image src="{{userimg}}"></image></view><view class="username">{{''+username+''}}</view><view class="loginbox"><block wx:if="{{!token||showbtn}}"><button class="loginbtn" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">微信快捷登录</button></block></view></view><view class="mylist"><block wx:for="{{listarr}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['navfn',['$0'],[[['listarr','',index]]]]]]]}}" class="{{[index==0?'listitem2':'listitem']}}" bindtap="__e"><view class="itemleft"><view class="{{['itemicon iconfont '+item.icon]}}"></view><view class="itemval">{{item.val}}</view></view><view class="itemright iconfont icon-jiantou"></view></view></block></view><view class="footbox"><block wx:for="{{foorarr}}" wx:for-item="item" wx:for-index="__i0__"><view data-event-opts="{{[['tap',[['footfn',['$0'],[[['foorarr','',__i0__]]]]]]]}}" class="{{[item.active?'footitem2':'footitem']}}" bindtap="__e"><view class="{{['itemicon iconfont '+item.icon]}}"></view><view class="itemval">{{item.val}}</view></view></block></view></view>