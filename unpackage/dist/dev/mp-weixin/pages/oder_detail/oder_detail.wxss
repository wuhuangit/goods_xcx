.container {
  width: 100%;
  background-color: #f7f7f7;
  padding-top: 30rpx;
  padding-bottom: 20rpx;
}
.container .adressbox,
.container .oderinfo {
  width: 90%;
  margin: 0rpx auto;
  background-color: white;
  padding: 15rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  margin-bottom: 20rpx;
}
.container .adressbox .adresstop,
.container .oderinfo .adresstop {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 36rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.container .adressbox .adresstop .usertel,
.container .oderinfo .adresstop .usertel {
  margin-left: 20rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #666;
}
.container .adressbox .adressval,
.container .oderinfo .adressval {
  margin-top: 10rpx;
  font-size: 30rpx;
}
.container .adressbox .adressval .infoitem,
.container .oderinfo .adressval .infoitem {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  padding: 10rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.container .adressbox .adressval .infoitem .itemname,
.container .oderinfo .adressval .infoitem .itemname {
  width: 120rpx;
}
.container .adressbox .adressval .infoitem .itemval,
.container .oderinfo .adressval .infoitem .itemval {
  margin-left: 30rpx;
}
.container .orderbox {
  width: 90%;
  margin: 0rpx auto;
  background-color: white;
  padding: 30rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  margin-top: 30rpx;
}
.container .orderbox .listitem {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  padding-bottom: 10px;
  margin-bottom: 10px;
  border-bottom: 1px solid #ccc;
}
.container .orderbox .listitem .goodsimg {
  width: 160rpx;
  height: 160rpx;
}
.container .orderbox .listitem .goodsimg image {
  width: 100%;
  height: 100%;
}
.container .orderbox .listitem .goodsinfo {
  width: 100rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20rpx;
}
.container .orderbox .listitem .goodsinfo .goodsname {
  width: 100%;
  font-size: 36rpx;
  font-weight: bold;
  height: 50rpx;
  line-height: 50rpx;
  color: #333333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.container .orderbox .listitem .goodsinfo .goodsnum {
  height: 60rpx;
  width: 100%;
  font-size: 28rpx;
  color: #C1C1C1;
  margin-top: 10rpx;
}
.container .orderbox .listitem .goodsinfo .goodsprice {
  width: 100%;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 0;
  text-align: left;
}
.container .orderbox .listitem .goodsinfo .goodsprice .priceyuan {
  color: #C1C1C1;
  color: #323232;
}
.container .orderbox .goodsprice {
  width: 100%;
  font-size: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
  text-align: right;
}
.container .orderbox .goodsprice .priceyuan {
  color: #C1C1C1;
  color: orangered;
}
.container .oderinfo {
  margin-top: 30rpx;
}
.container .footbox {
  width: 100%;
  background-color: white;
  position: fixed;
  bottom: 0;
  left: 0;
  padding: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.container .footbox .buybtn {
  width: 200rpx;
  background-color: orangered;
  color: white;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 32rpx;
}

